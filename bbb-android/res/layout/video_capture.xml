<?xml version="1.0" encoding="utf-8"?>

<org.mconf.bbb.android.video.VideoCaptureLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/video_capture_layout"
	android:layout_width="1px"
	android:layout_height="1px"
	android:visibility="visible"		
	android:layout_above="@id/audio_bar"
	android:weightSum="0.25"
	android:background="@color/video_background">
	<view class="org.mconf.bbb.android.video.VideoCapture" android:id="@+id/video_capture"
		android:layout_width="wrap_content" android:layout_height="wrap_content"/>

</org.mconf.bbb.android.video.VideoCaptureLayout>

